{% if bvbphotos.photos.length %}
  <section class="[ ig-thumb-section ] [ gap-bottom-900 ]">
    <div class="[ inner-wrapper ] [ sf-flow ]">
      <h2 class="[ post-list__heading ]">Photos</h2>
      <ul class="photos-list-home">
        {% for photo in bvbphotos
          .photos
          .slice(28, 37) %}
          <li class="photos-listitem">
            <a href="https://res.cloudinary.com/bradvsbrad/image/upload/w_1600/{{ photo.public_id }}.jpg" data-fancybox="images" data-caption="{{ photo.title }} {{ photo.caption }}" aria-label="{{ photo.title }} {{ photo.caption }}" data-slug="{{ photo.smugmug-slug }}" class="ig-thumb-link"><img src="https://res.cloudinary.com/bradvsbrad/image/upload/w_376,c_fill,g_auto,ar_1.667/{{ photo.public_id }}.jpg" alt="{{ photo.title }}" class="photos-thumb"></a>
          </li>
        </li>

      {% endfor %}

    </ul>
  </div>
</section>

{% endif %}
